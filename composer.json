{
    "name": "becklyn/deploy-message-generator",
    "description": "A symfony CLI tool that automatically produces beautiful deployment messages.",
    "license": "BSD-3-Clause",
    "type": "cli",
    "homepage": "https://github.com/Becklyn/gen-log",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/config": "^5.4.7 || ^6.0.7",
        "symfony/console": "^5.4.7 || ^6.0.7",
        "symfony/dotenv": "^5.4.5 || ^6.0.5",
        "symfony/http-client": "^5.4.7 || ^6.0.7",
        "symfony/notifier": "^5.4.6 || ^6.0.6",
        "symfony/process": "^5.4.7 || ^6.0.7",
        "symfony/runtime": "^5.4.7 || ^6.0.7",
        "symfony/slack-notifier": "^5.4.3 || ^6.0.3",
        "symfony/string": "^5.4.3 || ^6.0.3",
        "symfony/yaml": "^5.4.3 || ^6.0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^5.4.7 || ^6.0.7",
        "symfony/var-dumper": "^5.4.6 || ^6.0.6"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Becklyn\\DeployMessageGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\DeployMessageGenerator\\": "tests/"
        }
    },
    "bin": [
        "bin/deploy-message-generator"
    ],
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "runtime": {
            "class": "Symfony\\Component\\Runtime\\GenericRuntime"
        }
    }
}
