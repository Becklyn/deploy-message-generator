{
    "name": "becklyn/deploy-message-generator",
    "description": "A symfony CLI tool that automatically produces beautiful deployment messages.",
    "license": "BSD-3-Clause",
    "type": "cli",
    "homepage": "https://github.com/Becklyn/gen-log",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/config": "^5.4.7",
        "symfony/console": "^v5.4.7",
        "symfony/dotenv": "^v5.4.5",
        "symfony/http-client": "^v5.4.7",
        "symfony/notifier": "^v5.4.6",
        "symfony/process": "^v5.4.7",
        "symfony/runtime": "^v5.4.7",
        "symfony/slack-notifier": "^v5.4.3",
        "symfony/string": "^v5.4.3",
        "symfony/yaml": "^v5.4.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^v5.4.7",
        "symfony/var-dumper": "^v5.4.6"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Becklyn\\DeployMessageGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\DeployMessageGenerator\\": "tests/"
        }
    },
    "bin": [
        "bin/deploy-message-generator"
    ],
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "runtime": {
            "class": "Symfony\\Component\\Runtime\\GenericRuntime"
        }
    }
}
